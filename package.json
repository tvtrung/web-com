{
  "author": "FOURDIGIT",
  "name": "lampart",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "build:storybook": "build-storybook -c .configs/.storybook -s public",
    "build": "next build",
    "clean": "rm -rf .next node_modules/.cache build storybook-static out",
    "dev": "next dev",
    "export": "node src/helpers/export.js",
    "build:release": "next build && npm run export",
    "gen:component": "cross-env HYGEN_TMPLS=.configs/.hygen hygen new component",
    "gen:locale": "i18next -c ./.configs/.i18n/i18next-parser.config.js",
    "lint:script:update": "eslint ./ --ext .ts --ext .tsx --color --fix",
    "lint:script": "eslint ./ --ext .ts --ext .tsx --color",
    "lint:style:update": "stylelint ./src/**/*.{css,scss} --fix",
    "lint:style": "stylelint ./src/**/*.{css,scss}",
    "lint": "run-s lint:script",
    "prettier:check": "prettier -c **.{js,ts,tsx,css,scss}",
    "prettier:update": "prettier -w **.{js,ts,tsx,css,scss}",
    "start": "next start",
    "storybook": "start-storybook -c .configs/.storybook -p 6006 -s public --no-manager-cache"
  },
  "dependencies": {
    "@storybook/react": "^6.4.14",
    "@types/fslightbox-react": "^1.4.2",
    "@types/react": "17.0.34",
    "@types/react-gtm-module": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "animejs": "^3.2.1",
    "autoprefixer": "^10.4.0",
    "axios": "^1.3.2",
    "eslint-config-next": "12.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fs-extra": "^10.0.0",
    "fslightbox-react": "1.7.2",
    "html-react-parser": "^3.0.12",
    "lottie-react": "^2.2.1",
    "next": "^12.0.1",
    "next-i18next": "^10.1.0",
    "npm-run-all": "^4.1.5",
    "or": "^0.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-recaptcha-v3": "^1.10.1",
    "react-gtm-module": "^2.0.11",
    "react-hook-form": "^7.43.7",
    "react-scroll-parallax": "^3.0.1",
    "replace": "^1.2.1",
    "sass": "^1.43.2",
    "sharp": "^0.29.3",
    "swiper": "^7.0.9",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@fourdigit/eslint-config-fourdigit": "^2.0.0-alpha2",
    "@fourdigit/stylelint-config-fourdigit": "^2.0.0",
    "@next/bundle-analyzer": "^12.0.2",
    "@storybook/addon-essentials": "^6.4.14",
    "@storybook/builder-webpack5": "^6.4.14",
    "@storybook/manager-webpack5": "^6.4.14",
    "@types/animejs": "^3.1.4",
    "@whitespace/storybook-addon-html": "^5.0.0",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.30.0",
    "eslint-plugin-dollar-sign": "^1.0.2",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "glob": "^7.2.0",
    "husky": "^4",
    "hygen": "^6.1.0",
    "i18next-parser": "^5.4.0",
    "lint-staged": "^11.2.3",
    "node-sass-glob-importer": "^5.3.2",
    "postcss": "^8.4.4",
    "postcss-loader": "^6.2.1",
    "postcss-scss": "^4.0.2",
    "prettier": "^2.4.1",
    "react-share": "^4.4.0",
    "resolve-url-loader": "^4.0.0",
    "storybook-addon-paddings": "^4.2.1",
    "storybook-dark-mode": "^1.0.8",
    "stylelint": "^14.0.1",
    "stylelint-color-format": "^1.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^2.5.0",
    "stylelint-config-standard": "^23.0.0",
    "stylelint-config-standard-scss": "^2.0.1",
    "stylelint-order": "^5.0.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "webpack": "^5"
  },
  "peerDependencies": {
    "postcss": "^8.4.4",
    "postcss-loader": "^6.2.1",
    "postcss-scss": "^4.0.2"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --color",
      "prettier -c"
    ],
    "src/**/*.scss": [
      "stylelint --color",
      "prettier -c"
    ]
  },
  "engines": {
    "node": ">=14"
  },
  "resolutions": {
    "@storybook/**/*/browserslist": "^4.16.5",
    "@storybook/**/*/glob-parent": "^5.1.2",
    "@storybook/**/*/immer": "^9.0.6",
    "@storybook/**/*/set-value": "^4.0.1",
    "@storybook/**/*/trim": "^1.0.1",
    "i18next-parser/**/*/glob-parent": "^5.1.2"
  }
}
